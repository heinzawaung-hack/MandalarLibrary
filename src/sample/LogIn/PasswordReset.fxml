<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="308.0" prefWidth="534.0" style="-fx-background-color: #00d164;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LogIn.PasswordResetController">
   <children>
      <JFXButton fx:id="saveButton" layoutX="348.0" layoutY="220.0" onAction="#saveAction" prefHeight="35.0" prefWidth="104.0" style="-fx-background-color: #000;" text="Reset" textFill="#f8f8f8">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="cancelButton" layoutX="67.0" layoutY="220.0" onAction="#cancelAction" prefHeight="35.0" prefWidth="104.0" style="-fx-background-color: #000;" text="Cancel" textFill="#f8f8f8">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXButton>
      <JFXPasswordField fx:id="newPasswordField" focusColor="#070707" labelFloat="true" layoutX="162.0" layoutY="45.0" prefHeight="35.0" prefWidth="200.0" promptText="New Password">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXPasswordField>
      <JFXPasswordField fx:id="againNewPassword" focusColor="#070707" labelFloat="true" layoutX="162.0" layoutY="119.0" prefHeight="35.0" prefWidth="200.0" promptText="Enter Again New Password">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXPasswordField>
   </children>
</AnchorPane>
