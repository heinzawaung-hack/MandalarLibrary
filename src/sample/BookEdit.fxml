<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="187.0" prefWidth="631.0" styleClass="menuVoe" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BookEditController">
   <children>
      <JFXTextField fx:id="bookNameField" focusColor="#030303" labelFloat="true" layoutX="39.0" layoutY="40.0" prefHeight="29.0" prefWidth="164.0" promptText="Book Name" unFocusColor="BLACK">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="authorField" focusColor="#030303" labelFloat="true" layoutX="234.0" layoutY="40.0" prefHeight="29.0" prefWidth="164.0" promptText="Author" unFocusColor="BLACK">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="publisherField" focusColor="#030303" labelFloat="true" layoutX="425.0" layoutY="40.0" prefHeight="29.0" prefWidth="164.0" promptText="Publisher" unFocusColor="BLACK">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXTextField>
      <JFXRadioButton fx:id="trueRadio" layoutX="234.0" layoutY="86.0" selected="true" text="True">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="isAvail" />
         </toggleGroup>
      </JFXRadioButton>
      <JFXRadioButton fx:id="falseRadio" layoutX="316.0" layoutY="86.0" text="False" toggleGroup="$isAvail">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXRadioButton>
      <JFXButton fx:id="cancelButton" buttonType="RAISED" layoutX="112.0" layoutY="132.0" onAction="#cancelAction" prefHeight="26.0" prefWidth="91.0" text="Cancel">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="saveButton" layoutX="425.0" layoutY="132.0" onAction="#saveAction" prefHeight="26.0" prefWidth="91.0" text="Save">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
