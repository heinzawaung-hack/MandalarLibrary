<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="429.0" prefWidth="554.0" styleClass="relaxingRed" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LogIn.SignUpController">
   <children>
      <JFXTextField fx:id="userNameField" focusColor="#141414" labelFloat="true" layoutX="168.0" layoutY="45.0" prefHeight="33.0" prefWidth="201.0" promptText="Email">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="signInButton" buttonType="RAISED" layoutX="425.0" layoutY="19.0" onAction="#signInAction" prefHeight="33.0" prefWidth="96.0" stylesheets="@../style.css" text="LogIn">
         <font>
            <Font name="Arial" size="13.0" />
         </font></JFXButton>
      <JFXPasswordField fx:id="passwordField" focusColor="BLACK" labelFloat="true" layoutX="168.0" layoutY="122.0" prefHeight="33.0" prefWidth="201.0" promptText="Password">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXPasswordField>
      <JFXPasswordField fx:id="pinNumberField" focusColor="BLACK" labelFloat="true" layoutX="168.0" layoutY="190.0" prefHeight="33.0" prefWidth="201.0" promptText="Pin Number">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXPasswordField>
      <JFXTextField fx:id="addressField" focusColor="#070707" labelFloat="true" layoutX="168.0" layoutY="260.0" prefHeight="33.0" prefWidth="201.0" promptText="Address">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXTextField>
      <JFXRadioButton fx:id="femaleRadio" layoutX="269.0" layoutY="335.0" text="Female">
         <toggleGroup>
            <ToggleGroup fx:id="genderToggle" />
         </toggleGroup>
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXRadioButton>
      <JFXRadioButton fx:id="maleRadio" layoutX="168.0" layoutY="335.0" selected="true" text="Male" toggleGroup="$genderToggle">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXRadioButton>
      <JFXButton fx:id="signUpButton" buttonType="RAISED" layoutX="351.0" layoutY="368.0" onAction="#signUpAction" prefHeight="33.0" prefWidth="114.0" stylesheets="@../style.css" text="SignUp">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
